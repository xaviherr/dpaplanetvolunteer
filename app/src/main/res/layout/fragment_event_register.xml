<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.EventRegisterFragment">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="318dp"
        android:layout_height="156dp"
        android:layout_marginTop="41dp"
        app:layout_constraintBottom_toTopOf="@+id/txtRELugar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.602"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lmp" />

    <Button
        android:id="@+id/btnREGuardar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="52dp"
        android:text="Guardar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spREtipo" />

    <EditText
        android:id="@+id/txtREEvento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:hint="Evento"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtREDetalle"
        app:layout_constraintStart_toStartOf="@+id/txtREDetalle"
        app:layout_constraintTop_toBottomOf="@+id/txtRELugar" />

    <EditText
        android:id="@+id/txtREMonto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/spREtipo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spREtipo"
        app:layout_constraintTop_toTopOf="@+id/spREtipo"
        app:layout_constraintVertical_bias="0.38" />

    <EditText
        android:id="@+id/txtRELugar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="Lugar"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtREEvento"
        app:layout_constraintStart_toStartOf="@+id/txtREEvento"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <EditText
        android:id="@+id/txtREFin"
        android:layout_width="0dp"
        android:layout_height="46dp"
        android:layout_marginEnd="52dp"
        android:ems="10"
        android:hint="Fin"
        android:inputType="date"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtREInicio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtREInicio" />

    <Spinner
        android:id="@+id/spREtipo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toTopOf="@+id/btnREGuardar"
        app:layout_constraintEnd_toStartOf="@+id/txtREMonto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtREInicio" />

    <EditText
        android:id="@+id/txtRECelular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:hint="Celular"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtREInicio"
        app:layout_constraintStart_toStartOf="@+id/txtREDetalle"
        app:layout_constraintTop_toBottomOf="@+id/txtREDetalle" />

    <EditText
        android:id="@+id/txtREInicio"
        android:layout_width="0dp"
        android:layout_height="46dp"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:hint="Inicio"
        android:inputType="date"
        app:layout_constraintBottom_toTopOf="@+id/spREtipo"
        app:layout_constraintEnd_toStartOf="@+id/txtREFin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtRECelular" />

    <EditText
        android:id="@+id/txtREDetalle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="15dp"
        android:ems="10"
        android:hint="Detalle"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtRECelular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtREEvento" />

</androidx.constraintlayout.widget.ConstraintLayout>
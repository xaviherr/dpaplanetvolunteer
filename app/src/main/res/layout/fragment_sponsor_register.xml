<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.SponsorRegisterFragment">

    <EditText
        android:id="@+id/txtRAcelular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Celular"
        android:inputType="phone"
        app:layout_constraintBottom_toTopOf="@+id/txtRAcorreo"
        app:layout_constraintStart_toStartOf="@+id/txtRAdoc"
        app:layout_constraintTop_toBottomOf="@+id/txtRAdoc"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txtRAcontraseña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/checkprimero2"
        app:layout_constraintStart_toStartOf="@+id/txtRAcorreo"
        app:layout_constraintTop_toBottomOf="@+id/txtRAcorreo"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="318dp"
        android:layout_height="156dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/txtRACiudad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lmp"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <CheckBox
        android:id="@+id/checkSegundo2"
        android:layout_width="345dp"
        android:layout_height="34dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="48dp"
        android:text="Autorizo el uso de mi información para fines adicionales"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/btnRAcrearCuenta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/checkprimero2"
        app:layout_constraintTop_toBottomOf="@+id/checkprimero2"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txtRAcorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:ems="10"
        android:hint="Correo"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtRAcontraseña"
        app:layout_constraintStart_toStartOf="@+id/txtRAcelular"
        app:layout_constraintTop_toBottomOf="@+id/txtRAcelular"
        tools:ignore="TouchTargetSizeCheck" />

    <CheckBox
        android:id="@+id/checkprimero2"
        android:layout_width="329dp"
        android:layout_height="46dp"
        android:layout_marginEnd="24dp"
        android:text="Acepto las Políticas de privacidad y Condiciones de uso de LoveMyPlanet."
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/checkSegundo2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtRAcontraseña"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txtRAnombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtRAdoc"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/txtRACiudad"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btnRAcrearCuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-20dp"
        android:layout_marginBottom="41dp"
        android:text="Crear Cuenta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtRACiudad"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintStart_toStartOf="@+id/txtRACiudad"
        app:layout_constraintTop_toBottomOf="@+id/checkSegundo2" />

    <EditText
        android:id="@+id/txtRACiudad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:ems="10"
        android:hint="Ciudad"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtRAnombre"
        app:layout_constraintStart_toStartOf="@+id/txtRAnombre"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/txtRAdoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:ems="10"
        android:hint="RUC/DNI"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtRAcelular"
        app:layout_constraintStart_toStartOf="@+id/txtRAnombre"
        app:layout_constraintTop_toBottomOf="@+id/txtRAnombre"
        tools:ignore="TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>